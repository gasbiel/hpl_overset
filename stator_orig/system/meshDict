FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    meshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFile "stator.stl";

maxCellSize 20;

objectRefinements
{
	oversetRefinement
	{
		type				cone;
		cellSize			2.5;
		p0				(-17.5 0 0);
		p1				(17.5 0 0);
		radius0				50;
		radius1				50;
	}
}



